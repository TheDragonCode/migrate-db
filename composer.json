{
    "name": "dragon-code/migrate-db",
    "description": "Easy data transfer from one database to another",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "database",
        "migrating",
        "migration",
        "move",
        "transfer",
        "dragon-code",
        "dragon",
        "andrey-helldar"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@dragon-code.pro",
            "homepage": "https://dragon-code.pro"
        }
    ],
    "support": {
        "issues": "https://github.com/TheDragonCode/migrate-db/issues",
        "source": "https://github.com/TheDragonCode/migrate-db"
    },
    "funding": [
        {
            "type": "boosty",
            "url": "https://boosty.to/dragon-code"
        },
        {
            "type": "yoomoney",
            "url": "https://yoomoney.ru/to/410012608840929"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "dragon-code/contracts": "^2.15",
        "dragon-code/support": "^6.0",
        "illuminate/contracts": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/database": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "ext-pdo_mysql": "*",
        "ext-pdo_pgsql": "*",
        "doctrine/dbal": "^3.0 || ^4.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^9.6 || ^10.0 || ^11.0 || ^12.0"
    },
    "suggest": {
        "doctrine/dbal": "[For Laravel 8-10] Required to rename columns and drop SQLite columns (^3.5.1)."
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DragonCode\\MigrateDB\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "dragon-code/codestyler": true,
            "ergebnis/composer-normalize": true,
            "friendsofphp/php-cs-fixer": true,
            "symfony/thanks": true
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DragonCode\\MigrateDB\\ServiceProvider"
            ]
        }
    }
}
